for i = 1:3
    load(['data_',num2str(i),'.mat']);
    t_window = wal_T(end)-wal_T(1);
    fft_wal = abs(fft(wal_e));
    fft_band = abs(fft(wal_e_band));
    40_hz_index = (1/t_window)
    [~,breath_rate_wala(i)] = max(fft_wal(2:floor(end/2)));
    [~,breath_rate_band(i)] = max(fft_band(2:floor(end/2)));
    breath_rate_wala(i) = breath_rate_wala(i).*(1/t_window)*60;
    breath_rate_band(i) = breath_rate_band(i).*(1/t_window)*60;
%     plot(abs(fft(wal_e)),'b'), hold on, plot(abs(fft(wal_e_band)),'r'), hold off;
end


